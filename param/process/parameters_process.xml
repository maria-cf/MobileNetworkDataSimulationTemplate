<?xml version="1.0"?>
	<process_parameters>
		<geolocation>
			<model_name>HMM</model_name>
			<emission_model>RSS_TA</emission_model>
			<transition_model>HMMrectangle</transition_model>
			<prior>network</prior>
			<params>
				<vmax_ms>45</vmax_ms>
			</params>
		</geolocation>
		<deduplication>
			<method>1to1</method>
			<lambda>0.85/0.15</lambda>
		</deduplication>
		<aggregation>
			<nSim>10000</nSim>
			<prefix_postLoc>postLocProb_HMM_RSS-network</prefix_postLoc>
			<prefix_postLocJoint>postLocJointProb_HMM_RSS-network</prefix_postLocJoint>
		</aggregation>
		<inference>
			<model_name>BetaNegBin</model_name>
			<ci_prob>0.95</ci_prob>
			<ci_method>HDI</ci_method>
		</inference>
	</process_parameters>